#! /bin/sh

touch $DATA_DIR/$CUR_TEST.gen

run_binary "./ffspart" "-s 0x100 -c 10 -i $DATA_DIR/$CUR_TEST.in -p $DATA_DIR/$CUR_TEST.gen --allow_empty"
if [ "$?" -ne 0 ] ; then
	fail_test
fi

if ! cmp $DATA_DIR/$CUR_TEST.out $DATA_DIR/$CUR_TEST.gen ; then
	echo "Output differs"
	fail_test
fi

pass_test
