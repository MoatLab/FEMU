    SECTION .text

;------------------------------------------------------------------------------
;  VOID *
;  EFIAPI
;  InternalMemSetMem64 (
;    IN VOID   *Buffer,
;    IN UINTN  Count,
;    IN UINT64 Value
;    )
;------------------------------------------------------------------------------
global ASM_PFX(InternalMemSetMem64)
ASM_PFX(InternalMemSetMem64):
    mov     eax, [esp + 4]
    mov     ecx, [esp + 8]
    movq    mm0, [esp + 12]
    mov     edx, eax
.0:
    movq    [edx], mm0
    add     edx, 8
    loop    .0
    ret

