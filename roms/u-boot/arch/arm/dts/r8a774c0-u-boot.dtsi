#include "r8a779x-u-boot.dtsi"

/ {
	soc {
		rpc: rpc@ee200000 {
			compatible = "renesas,rcar-gen3-rpc", "renesas,rpc-r8a774c0";
			reg = <0 0xee200000 0 0x100>, <0 0x08000000 0 0x04000000>;
			clocks = <&cpg CPG_MOD 917>;
			bank-width = <2>;
			status = "disabled";
		};
	};
};

