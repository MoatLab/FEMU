#include "am33xx-u-boot.dtsi"

/ {
	chosen {
		#address-cells = <1>;
		#size-cells = <1>;

		bootargs = "console=ttyO0,115200 earlyprintk";
		stdout-path = &uart0;
	};

	ocp {
		u-boot,dm-pre-reloc;
	};
};

&i2c0 {
	u-boot,dm-pre-reloc;
};

&uart0 {
	u-boot,dm-pre-reloc;
};

&mmc1 {
	u-boot,dm-pre-reloc;
	cd-gpios = <&gpio0 6 GPIO_ACTIVE_LOW>;
};
