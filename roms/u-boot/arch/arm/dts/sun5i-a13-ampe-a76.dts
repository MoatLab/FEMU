 */

/dts-v1/;
#include "sun5i-a13.dtsi"

/ {
	model = "Ampe A76";
	compatible = "ampe,a76", "allwinner,sun5i-a13";

	aliases {
		serial0 = &uart1;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart1_pins_b>;
	status = "okay";
};
