/ {
	smbios {
		compatible = "u-boot,sysinfo-smbios";

		smbios {
			system {
				product = "uDPU";
			};

			baseboard {
				product = "uDPU";
			};

			chassis {
				product = "uDPU";
			};
		};
	};
};

&spi0 {
	u-boot,dm-pre-reloc;

	spi-flash@0 {
		u-boot,dm-pre-reloc;
	};
};

&sdhci1 {
	u-boot,dm-pre-reloc;
};
