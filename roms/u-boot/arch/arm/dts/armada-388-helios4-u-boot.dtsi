&eth0 {
	phy-reset-gpios = <&gpio0 19 GPIO_ACTIVE_LOW>;
};

&spi1 {
	u-boot,dm-spl;
	spi-flash@0 {
		u-boot,dm-spl;
	};
};

&w25q32 {
	u-boot,dm-spl;
};

&gpio0 {
	u-boot,dm-spl;
};

&ahci0 {
	u-boot,dm-spl;
};

&ahci1 {
	u-boot,dm-spl;
};

&sdhci {
	u-boot,dm-spl;
};

&i2c0 {
	u-boot,dm-spl;

	eeprom@52 {
		u-boot,dm-spl;
	};

	eeprom@53 {
		u-boot,dm-spl;
	};
};
