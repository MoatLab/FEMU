/ {
	aliases {
		usb0 = &usbotg;
		video0 = &ipu1;
	};

	soc {
		u-boot,dm-spl;
		u-boot,dm-pre-reloc;

		aips-bus@2000000 {
			u-boot,dm-spl;
			spba-bus@2000000 {
				u-boot,dm-spl;
			};
		};

		aips-bus@2100000 {
			u-boot,dm-spl;
		};
	};
};

&gpio1 {
	u-boot,dm-spl;
};

&iomuxc {
	u-boot,dm-spl;
};

&ipu1 {
	u-boot,dm-pre-reloc;
};
