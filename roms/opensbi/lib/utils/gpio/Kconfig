menu "GPIO Support"

config FDT_GPIO
	bool "FDT based GPIO drivers"
	depends on FDT
	select GPIO
	default n

if FDT_GPIO

config FDT_GPIO_DESIGNWARE
	bool "DesignWare GPIO driver"
	default n

config FDT_GPIO_SIFIVE
	bool "SiFive GPIO FDT driver"
	default n

config FDT_GPIO_STARFIVE
	bool "StarFive GPIO FDT driver"
	default n
endif

config GPIO
	bool "GPIO support"
	default n

endmenu
